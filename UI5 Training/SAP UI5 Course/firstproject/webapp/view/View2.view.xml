<mvc:View controllerName="firstproject.controller.View1"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m">
 <Shell id="shell1">
 <App id="app1"> 
<Panel id="panel1" headerText="Buttons">
        <Button id="button1" text="{customModel>/key}" press=".onChangeText" />
        <Input
			id="Input1"
			class="sapUiSmallMarginBottom"
			placeholder="Change Button text here"
			valueStateText="Name must not be empty. Maximum 10 characters."
			value="{customModel>/key}"/>
        </Panel>
   </App>
</Shell>
</mvc:View>
